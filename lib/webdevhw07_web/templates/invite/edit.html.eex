<%= if current_user_id?(@conn, @invite.user_id) do %>
    <h1>Edit Invite</h1>

    <%= render "form.html", Map.put(assigns, :action, Routes.invite_path(@conn, :update, @invite)) %>

    <span><%= link "Back", to: Routes.invite_path(@conn, :index) %></span>
<%else %>
 <div class="container">
    <div class="row">
      <div class="col">
        <h1 class="align-text-center">You don't have access to this</h1>
      </div>
    </div>
  </div>
<%end %>
